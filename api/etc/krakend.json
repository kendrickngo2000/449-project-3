{
    "$schema": "https://www.krakend.io/schema/v2.4/krakend.json",
    "version": 3,
    "endpoints": [
      {
        "endpoint": "/api/register",
        "method": "POST",
        "backend": [
            {
            "url_pattern": "/register",
            "method": "POST",
            "host": [
                "http://localhost:5200"
            ],
            "extra_config": {
              "backend/http": {
                  "return_error_details": "backend"
            }
          }
          }
        ]
      },
      {
        "endpoint": "/api/login",
        "method": "POST",
        "backend": [
            {
            "url_pattern": "/login",
            "method": "POST",
            "host": [
                "http://localhost:5200"
            ],
            "extra_config": {
              "backend/http": {
                  "return_error_details": "backend"
              }
            }
          }
        ],
        "extra_config": {
          "auth/signer": {
              "alg": "HS256",
              "kid": "sim2",
              "keys_to_sign": [
                  "access_token",
                  "refresh_token"
              ],
              "jwk_local_path": "symmetric.json",
              "disable_jwk_security": true
          }
        }
      },
      {
        "endpoint": "/api/all_classes",
