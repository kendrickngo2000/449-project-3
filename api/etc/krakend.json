        ],
        "extra_config": {
          "auth/validator": {
              "alg": "HS256",
              "jwk_local_path": "symmetric.json",
              "roles_key": "roles",
                    "roles": ["student"],
              "operation_debug": true,
              "disable_jwk_security": true,
              "cache": false
          }
        }
